{
    "name": "@aerogel/vite",
    "version": "0.1.2",
    "type": "module",
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/aerogel-vite.d.ts",
            "default": "./dist/aerogel-vite.js"
        },
        "./virtual": {
            "types": "./dist/virtual.d.ts"
        }
    },
    "files": [
        "src",
        "dist"
    ],
    "license": "MIT",
    "author": "Noel De Martin",
    "repository": "github:NoelDeMartin/aerogel",
    "scripts": {
        "build": "vite build && cp src/types/virtual.d.ts dist/virtual.d.ts",
        "lint": "noeldemartin-lint src",
        "test": "vitest --run",
        "test:ci": "vitest --run --reporter verbose",
        "verify": "noeldemartin-verify"
    },
    "dependencies": {
        "@noeldemartin/utils": "catalog:",
        "@tailwindcss/vite": "^4.1.4",
        "@vitejs/plugin-vue": "^5.2.3",
        "image-size": "^2.0.2",
        "mustache": "^4.2.0",
        "rollup": "^4.54.0",
        "unplugin-vue-components": "^28.4.1",
        "vite-plugin-pwa": "^0.21.2",
        "workbox-build": "^7.4.0"
    },
    "devDependencies": {
        "@arethetypeswrong/cli": "catalog:",
        "@noeldemartin/scripts": "catalog:",
        "eslint": "catalog:",
        "publint": "catalog:"
    },
    "eslintConfig": {
        "extends": [
            "@noeldemartin/eslint-config-typescript"
        ]
    }
}
