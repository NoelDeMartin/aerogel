{
    "name": "@aerogel/cypress",
    "description": "Aerogel Cypress plugin",
    "version": "0.0.0",
    "main": "dist/aerogel-cypress.cjs.js",
    "module": "dist/aerogel-cypress.esm.js",
    "types": "dist/aerogel-cypress.d.ts",
    "sideEffects": false,
    "scripts": {
        "build": "rm dist -rf && npm run build:js && npm run build:types && npm run build:plugin",
        "build:js": "noeldemartin-build-javascript",
        "build:plugin": "esbuild src/plugin.ts --format=cjs --bundle --external:soukai --external:soukai-solid --external:@inrupt/solid-client-authn-core --external:@simonsmith/cypress-image-snapshot/plugin > dist/plugin.js",
        "build:types": "noeldemartin-build-types && sed -i \"1i\\/// <reference types=\\\"@simonsmith/cypress-image-snapshot/command\\\" />\" ./dist/aerogel-cypress.d.ts",
        "publish-next": "noeldemartin-publish-next",
        "lint": "noeldemartin-lint src"
    },
    "engines": {
        "node": ">=18.x"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/NoelDeMartin/aerogeljs.git"
    },
    "keywords": [
        "javascript",
        "aerogel",
        "cypress"
    ],
    "author": "Noel De Martin",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/NoelDeMartin/aerogeljs/issues"
    },
    "peerDependencies": {
        "cypress": "*"
    },
    "dependencies": {
        "@noeldemartin/utils": "0.4.0-next.2549ce58d5c94a6009ecdd37d6fe2fcd76b6038e",
        "@simonsmith/cypress-image-snapshot": "^8.1.2"
    },
    "devDependencies": {
        "esbuild": "^0.19.4"
    }
}
