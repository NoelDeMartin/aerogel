{
    "name": "playground",
    "private": true,
    "scripts": {
        "build": "vite build",
        "cy:dev": "concurrently --kill-others \"npm run test:serve-app\" \"npm run test:serve-pod\" \"npm run cy:open\"",
        "cy:open": "cypress open --e2e --browser chromium",
        "cy:run": "cypress run",
        "cy:test": "start-server-and-test test:serve-app http-get://localhost:5001 test:serve-pod http-get://localhost:4000 cy:run",
        "dev": "vite",
        "dev:serve-pod": "cd ../solid-server && npx community-solid-server -c @css:config/file-no-setup.json -p 4000 -f ./data",
        "histoire:dev": "histoire dev",
        "histoire:prod": "histoire build",
        "lint": "noeldemartin-lint src",
        "test:serve-app": "vite --port 5001",
        "test:serve-pod": "cd ../solid-server && npx community-solid-server -p 4000 -l warn"
    },
    "dependencies": {
        "@aerogel/core": "*",
        "@aerogel/plugin-i18n": "*",
        "@aerogel/plugin-offline-first": "*",
        "@aerogel/plugin-routing": "*",
        "@aerogel/plugin-solid": "*",
        "@aerogel/plugin-soukai": "*",
        "@noeldemartin/solid-utils": "0.3.0-next.5176c3d9efdc3cf9d06772d0c55c24b2eb49893a",
        "@noeldemartin/utils": "0.4.0-next.ac00beaecf32bb02ed8e335225d7948d946d73bd",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/typography": "^0.5.9",
        "soukai": "0.5.1-next.6ceeddd517fac8fd180856fb3867cc33c1fa1b0b",
        "soukai-solid": "0.5.1-next.17ce9fccdbfa3d99bd1718e9aaf169cea189ee8a",
        "tailwindcss": "^3.3.2"
    },
    "devDependencies": {
        "@aerogel/cli": "*",
        "@aerogel/cypress": "*",
        "@aerogel/vite": "*",
        "@histoire/plugin-vue": "^0.16.1",
        "@iconify/json": "^2.2.80",
        "@inrupt/solid-client-authn-core": "^1.17.2",
        "autoprefixer": "^10.4.14",
        "concurrently": "^8.0.1",
        "eslint-plugin-vue": "^9.11.0",
        "histoire": "^0.16.2",
        "postcss": "^8.4.23",
        "postcss-pseudo-classes": "^0.2.1",
        "start-server-and-test": "^2.0.0",
        "unplugin-icons": "^0.16.3"
    }
}
